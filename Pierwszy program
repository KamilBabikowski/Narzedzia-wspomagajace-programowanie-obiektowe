public class Dwaklasa {
    String fname = "John"; 
    String lname = "Doe";
    int age = 28;

    public static void main(String[] args) {
        Dwaklasa myObj = new Dwaklasa();
        System.out.println("Name: " + myObj.fname + " " + myObj.lname);
        System.out.println("Age: " + myObj.age);
    }
}

